{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.17.3"
  },
  "scripts": {
    "start": "vite --port $PORT",
    "dev": "npm-run-all --parallel tailwind-watch vite-dev",
    "tailwind-watch": "npx tailwindcss -i ./src/assets/tailwind.css -o ./src/index.css --watch",
    "vite-dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && ntl deploy --prod"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@headlessui/vue": "^1.4.3",
    "@heroicons/vue": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.25.0",
    "vite-plugin-vue-env": "^1.0.6",
    "vue": "^3.0.0",
    "vue-lang-router": "^2.1.0",
    "vue-router": "^4.0.0-0",
    "vue-sidebar-menu": "^5.1.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "netlify-cli": "^9.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "sass": "^1.49.0",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^3.0.15",
    "vite": "^2.7.2"
  }
}
